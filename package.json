{
  "name": "simple-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run simpleTest:serve-ssr",
    "serve:ssr": "node dist/simpleTest/server/main.js",
    "build:ssr": "ng build && ng run simpleTest:server",
    "build:production": "ng build && ng run themis:server && cp lambda.js ./dist",
    "prerender": "ng run simpleTest:prerender",
    "clean": "rm -rf dist",
    "precompress_for_manual_upload": "rm -rf node_modules && npm ci --only=prod --no-progress",
    "compress_dist": "zip -rFS sam/deploy.zip dist/* node_modules",
    "start:sam": "sam local start-lambda",
    "lambda": "cd sam && sam local invoke angularHandler"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@nguniversal/express-engine": "^13.1.0",
    "@vendia/serverless-express": "^4.8.0",
    "angular-oauth2-oidc": "^13.0.1",
    "crypto-js": "^4.1.1",
    "express": "^4.15.2",
    "qrcode": "^1.5.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.4",
    "@angular/cli": "~13.3.4",
    "@angular/compiler-cli": "~13.3.0",
    "@nguniversal/builders": "^13.1.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.4",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.2"
  }
}
